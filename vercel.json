{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "x-access-key-server",
          "value": "$ACCESS_KEY"
        },
        {
          "key": "x-gemini-keys-pool-server",
          "value": "$GEMINI_API_KEYS"
        }
      ]
    }
  ],
  "functions": {
    "api/vercel_index.js": {
      "runtime": "edge"
    }
  }
}
